{
    "name": "seongbae/keywordrank",
    "description": "A Laravel package for getting keyword positions from Google",
    "keywords": ["laravel", "google", "keywords", "search", "console", "rank"],
    "authors": [
        {
            "name": "Seong Bae",
            "email": "seong@lnidigital.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=5.6.4",
        "illuminate/support": "~5.1"
    },
    
    "require-dev": {
        "phpunit/phpunit": "~3.0",
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "seongbae\\KeywordRank\\": "src/"
        },
        "files": [
            "src/Libs/simple_html_dom.php",
            "src/Helpers/KeywordRankHelper.php"
        ]
    },
    "extra": {
        "component": "package",
        "frameworks": ["Laravel 5.5"],
        "laravel": {
            "providers": [
                "seongbae\\KeywordRank\\KeywordRankServiceProvider"
            ]            
        }
    },
    "minimum-stability": "dev"
}
