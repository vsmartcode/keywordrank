{
    "name": "vsmartcode/keywordrank",
    "description": "A Laravel package for getting keyword positions from Google",
    "keywords": ["laravel", "google", "keywords", "search", "console", "rank"],
    "authors": [
        {
            "name": "Seong Bae",
            "email": "seong@lnidigital.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "illuminate/support": "4.*|5.*|6.*|7.*"
    },
    
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "orchestra/testbench": "3.6.*|3.7.*|3.8.*|~6.0"
    },
    "autoload": {
        "psr-4": {
            "vsmartcode\\KeywordRank\\": "src/"
        },
        "files": [
            "src/Libs/simple_html_dom.php",
            "src/Helpers/KeywordRankHelper.php"
        ]
    },
    "extra": {
        "component": "package",
        "frameworks": ["Laravel"],
        "laravel": {
            "providers": [
                "vsmartcode\\KeywordRank\\KeywordRankServiceProvider"
            ]            
        }
    },
    "minimum-stability": "dev"
}
